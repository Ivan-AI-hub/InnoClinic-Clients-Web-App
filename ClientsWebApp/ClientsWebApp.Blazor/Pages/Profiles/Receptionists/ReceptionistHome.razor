@page "/receptionists/home"
@attribute [Authorize(Roles = "Admin")]
@inherits CancellableComponent
@if(Receptionist != null)
{
    <ReceptionistCard Receptionist="Receptionist"/>
    <MudButton OnClick="NavigateToEditPage" Color="Color.Primary" Variant="Variant.Filled">@_localizer.GetString(LocalizationType.EditButtonValue)</MudButton>
    <MudButton OnClick="NavigateToDeletePage" Color="Color.Error" Variant="Variant.Filled">@_localizer.GetString(LocalizationType.DeleteButtonValue)</MudButton>

}
else
{
    <Loading/>
}