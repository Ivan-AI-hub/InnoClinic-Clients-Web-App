@page "/receptionists/home"
@attribute [Authorize(Roles = "Admin")]
@inherits CancellableComponent
@if(!IsLoading)
{
    <ReceptionistCard Receptionist="Receptionist"/>
    <MudButton OnClick="NavigateToEditPage" Color="Color.Primary" Variant="Variant.Filled">Edit</MudButton>
    <MudButton OnClick="NavigateToDeletePage" Color="Color.Error" Variant="Variant.Filled">Delete</MudButton>

}
else
{
    <Loading/>
}